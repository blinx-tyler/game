  "dependencies": {
    "body-parser": "^1.17.1",
    "ejs-locals": "^1.0.2",
    "express": "^4.14.1",
    "morgan": "^1.8.1",
    "nconf": "^0.8.4",
    "winston": "^2.3.1",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "cross-env": "^3.2.4"
  }
